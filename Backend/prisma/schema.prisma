generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgresql://neondb_owner:npg_dvtXDl1R7yTk@ep-royal-night-a1cfbdfv-pooler.ap-southeast-1.aws.neon.tech/neondb?sslmode=require&channel_binding=require"
}

model Asset {
  id         Int                      @id @default(autoincrement())
  type       String
  category   String
  status     String
  Details    String
  Images     String
  line       Unsupported("geometry")?
  point      Unsupported("geometry")?
  kpin       String
  Company    String                   @default("Other")
  pfrequency Int                      @default(0)
}

model ModeComplaint {
  id          Int                    @id @default(autoincrement())
  complaintId String                 @unique
  userId      String
  embedding   Unsupported("vector")?
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model spatial_ref_sys {
  srid      Int     @id
  auth_name String? @db.VarChar(256)
  auth_srid Int?
  srtext    String? @db.VarChar(2048)
  proj4text String? @db.VarChar(2048)
}
model Language {
  id       Int    @id @default(autoincrement())
  kpin     String @unique
  hindi    Int @default(0)
  telugu   Int @default(0)
  tamil    Int @default(0)
  kannada   Int @default(0)
  urdu     Int @default(0)
  marathi  Int @default(0)
  malayalam  Int @default(0)
}

model Location {
  id        Int    @id @default(autoincrement())
  kpin      String @unique
  latitude  String
  longitude String
}
